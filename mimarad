#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
int main()
{
	float array[100];
	int i = 0, j = 0;;
	float sum=0;
	char remaining[100];
	char *temp= "a";
	while (scanf("%fl", &array[i]) != 0)
	{
		sum = sum + array[i];
		i = i + 1;
	}
	printf("%.3f\n", sum);
	while (scanf("%[^\n]s", remaining) != 0)
	{
		printf("%s ", remaining);
	}
	return 0;
}
